{
    "manifest_version": 2,
    "name": "__MSG_name__",
    "description": "__MSG_description__",
    "version": "1.0",
    "default_locale": "en",
    "browser_action": {
        "default_icon": {
            "16": "images/icon16.png",
            "32": "images/icon32.png",
            "64": "images/icon64.png",
            "128": "images/icon128.png"
        }
    },
    "permissions": [
        "alarms",
        "downloads",
        "nativeMessaging",
        "tabs",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["URI.js","background.js"]
    },
    "web_accessible_resources": [
        "block.html"
    ],
    "update_url": "https://aapi-sec-epp.xiaojukeji.com/browser-extension/chrome/manifest.xml"
}
